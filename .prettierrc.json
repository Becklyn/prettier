{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "printWidth": 100,
    "singleQuote": false,
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "importOrder": [
        "^react(.*)$",
        "^next(.*)$",
        "^@mayd/(.*)$",
        "^[^.@].*",
        "^@(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
